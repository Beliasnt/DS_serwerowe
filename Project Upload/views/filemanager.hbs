<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Icon</th>
            <th>Name</th>
            <th>Size</th>
            <th>type</th>
            <th>-</th>
            <th>-</th>
            <th>-</th>
            <th>-</th>
        </tr>
    </thead>
    <tbody>
        {{#each files}}
        <tr>
            <td>{{id}}</td>
            <td><img src="{{icon}}" alt="{{icon}}"></td>
            <td>{{name}}</td>
            <td>{{size}}</td>
            <td>{{type}}</td>
            <td><a href="/show?id={{id}}">SHOW</a></td>
            <td><a href="/info?id={{id}}">INFO</a></td>
            <td><a href="/download?id={{id}}">DOWNLOAD</a></td>
