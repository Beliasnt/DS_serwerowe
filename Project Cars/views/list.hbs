<h2 class=" m-5"> List cars page</h2>
<table class="table m-lg-5">
    <thead>
        <tr class=" table-info">
            <th>Id</th>
            <th>Ubezpieczony</th>
            <th>Benzyna</th>
            <th>Uszkodzony</th>
            <th>Napęd 4x4</th>
            <th>INFO</th>
        </tr>
    </thead>
    <tbody>
        {{#each lista}}
        <tr class=" table-warning">
            <th>{{_id}}</th>
            <th>{{ubezpieczony}}</th>
            <th>{{benzyna}}</th>
            <th>{{uszkodzony}}</th>
            <th>{{napęd4x4}}</th>
            <th>
                <input class="btn btn-info me-2" type="button" value="INFO"
                    onclick="confirm('id: {{_id}} \n ubezpieczony: {{ubezpieczony}} \n benzyna: {{benzyna}} \n uszkodzony: {{uszkodzony}} \n naped4x4: {{napęd4x4}}')">
            </th>
        </tr>
        {{/each}}
    </tbody>
</table>